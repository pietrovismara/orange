{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "semi": 0,
        "constructor-super": 1,
        "no-this-before-super": 2,        
        "complexity": 2,
        "eqeqeq": 2,
        "no-else-return": 2,
        "no-implicit-globals": 2,
        "no-redeclare": 2,
        "no-useless-concat": 1,
        "no-unused-vars": 2,
        "no-shadow": 2,
        "block-spacing": 1,
        "camelcase": 1,
        "brace-style": 1,
        "comma-spacing": 1,
        "array-bracket-spacing": 1,
        "key-spacing": 1,
        "keyword-spacing": 1,
        "no-negated-condition": 1,
        "semi-spacing": 1,
        "space-infix-ops": 1
    },
    "complexity": ["error", { "max": 2 }],
    "eqeqeq": "error",
    "no-else-return": "error",
    "no-implicit-globals": "error",
    "no-redeclare": "error",
    "no-useless-concat": "error",
    "no-unused-vars": "error",
    "no-shadow": "error",
    "block-spacing": "error",
    "camelcase": "error",
    "brace-style": "error",
    "comma-spacing": "error",
    "array-bracket-spacing": ["error", "always"],
    "key-spacing": ["error", { "beforeColon": false }],
    "keyword-spacing": ["error", { "before": true, "after": true }],
    "no-negated-condition": "error",
     "semi-spacing": ["error", {"before": false, "after": true}],
     "space-infix-ops": "error",
}
